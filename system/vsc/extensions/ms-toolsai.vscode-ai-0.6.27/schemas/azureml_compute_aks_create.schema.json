{
    "type": "object",
    "description": "Azure Kubernetes Service",
    "properties": {
        "properties": {
            "type": "object",
            "description": "Compute properties",
            "properties": {
                "computeType": {
                    "type": "string",
                    "description": "The type of the compute",
                    "const": "AKS"
                },
                "properties": {
                    "type": "object",
                    "description": "AKS Compute properties",
                    "properties": {
                        "agentVmSize": {
                            "type": "string",
                            "description": "Agent virtual machine size"
                        },
                        "agentCount": {
                            "type": "integer",
                            "description": "Number of agents",
                            "minimum": 1
                        },
                        "aksNetworkingConfiguration": {
                            "type": "object",
                            "description": "[Optional] AKS networking configuration for vnet",
                            "properties": {
                                "dnsServiceIP": {
                                    "type": "string",
                                    "description": "An IP address assigned to the Kubernetes DNS service. It must be within the Kubernetes service address range specified in serviceCidr."
                                },
                                "dockerBridgeCidr": {
                                    "type": "string",
                                    "description": "A CIDR notation IP range assigned to the Docker bridge network. It must not overlap with any Subnet IP ranges or the Kubernetes service address range."
                                },
                                "serviceCidr": {
                                    "type": "string",
                                    "description": "A CIDR notation IP range from which to assign service cluster IPs. It must not overlap with any Subnet IP ranges."
                                },
                                "subnetId": {
                                    "type": "string",
                                    "description": "Virtual network subnet resource ID the compute nodes belong to"
                                }
                            }
                        },
                        "clusterFqdn": {
                            "type": "string",
                            "description": "[Optional] Cluster full qualified domain name"
                        },
                        "clusterPurpose": {
                            "type": "string",
                            "description": "[Optional] Targeted usage of the cluster. This is used to provision AzureML components to\nensure the desired level of fault-tolerance and QoS. AksCompute.ClusterPurpose class is provided for\nconvenience of specifying available values. More detailed information of these values and their use cases\ncan be found here: https://aka.ms/azureml-create-attach-aks"
                        },
                        "sslConfiguration": {
                            "type": "object",
                            "description": "[Optional] SSL configuration",
                            "properties": {
                                "cert": {
                                    "type": "string",
                                    "description": "Cert data"
                                },
                                "cname": {
                                    "type": "string",
                                    "description": "CNAME of the cert"
                                },
                                "key": {
                                    "type": "string",
                                    "description": "Key data"
                                },
                                "status": {
                                    "type": "string",
                                    "description": "Enable or disable ssl for scoring",
                                    "default": "Disabled",
                                    "enum": [
                                        "Disabled",
                                        "Enabled",
                                        "Auto"
                                    ]
                                },
                                "leafDomainLabel": {
                                    "type": "string"
                                },
                                "overwriteExistingDomain": {
                                    "type": "boolean",
                                    "default": "false"
                                }
                            }
                        }
                    },
                    "required": [
                        "agentVmSize",
                        "agentCount"
                    ]
                }
            },
            "required": [
                "computeType",
                "properties"
            ]
        }
    },
    "required": [
        "properties"
    ]
}
