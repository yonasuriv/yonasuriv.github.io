{
	"name": "vscode-ai-remote",
	"displayName": "Azure Machine Learning - Remote",
	"description": "This extension is used by the Azure Machine Learning Extension",
	"version": "0.5.0",
	"publisher": "ms-toolsai",
	"engines": {
		"vscode": "^1.55.0"
	},
	"icon": "icons/logo.png",
	"preview": true,
	"categories": [
		"Azure",
		"Other"
	],
	"keywords": [
		"Azure Machine Learning",
		"AML",
		"Azure ML",
		"Deep Learning"
	],
	"enableProposedApi": true,
	"repository": {
		"url": "https://github.com/Microsoft/vscode-tools-for-ai"
	},
	"extensionKind": [
		"workspace",
		"ui"
	],
	"extensionPack": [
		"ms-toolsai.vscode-ai"
	],
	"activationEvents": [
		"onResolveRemoteAuthority:amlext",
		"onCommand:vscodeai.azureml.computeInstance.remote.uninstallVSCodeServer",
		"onCommand:vscodeai.azureml.computeInstance.remote.killVSCodeServer",
		"onCommand:vscodeai.azureml.computeInstance.remote.installWebsocketServer",
		"onCommand:vscodeai.azureml.computeInstance.remote.killWebsocketServer",
		"onCommand:vscodeai.azureml.computeInstance.remote.diagnosticReport"
	],
	"main": "./out/src/extension.js",
	"contributes": {
		"resourceLabelFormatters": [
			{
				"scheme": "vscode-remote",
				"authority": "amlext+*",
				"formatting": {
					"label": "${path}",
					"separator": "/",
					"tildify": true,
					"workspaceSuffix": "Azure ML"
				}
			}
		],
		"configuration": {
			"type": "object",
			"title": "%configuration.title%",
			"properties": {
				"azureML - Remote.logLevel.file": {
					"description": "%logLevel.file.description%",
					"type": "string",
					"enum": [
						"error",
						"warn",
						"info",
						"verbose",
						"debug",
						"silly"
					],
					"default": "debug"
				},
				"azureML - Remote.logLevel.channel": {
					"description": "%logLevel.channel.description%",
					"type": "string",
					"enum": [
						"error",
						"warn",
						"info",
						"verbose",
						"debug",
						"silly"
					],
					"default": "info"
				},
				"azureML - Remote.logLevel.window": {
					"description": "%logLevel.window.description%",
					"type": "string",
					"enum": [
						"error",
						"warn",
						"info",
						"verbose",
						"debug",
						"silly"
					],
					"default": "info"
				},
				"azureML - Remote.telemetry": {
					"type": "boolean",
					"default": true,
					"description": "%telemetry.description%"
				},
				"azureML - Remote.authenticationScope": {
					"type": "string",
					"default": "Default",
					"description": "%authenticationScope.description%",
					"enum": [
						"Default",
						"User Impersonation"
					]
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "cross-env NODE_ENV=production webpack --mode production",
		"build": "webpack --mode development --info-verbosity none",
		"watch": "webpack -w --mode development --info-verbosity none",
		"package": "vsce package",
		"tslint": "tslint --project ./",
		"compile": "tsc -p ./",
		"prettier-fix": "prettier src/**/*.ts --write"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.4",
		"@types/glob": "^7.1.3",
		"@types/lodash": "^4.14.165",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/node-fetch": "^2.5.7",
		"@types/request": "^2.48.5",
		"@types/uuid": "^8.3.0",
		"@types/vscode": "^1.54.0",
		"@types/ws": "^7.4.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"cross-env": "^7.0.3",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"ts-loader": "^8.0.3",
		"tslint": "^6.1.3",
		"tslint-config-prettier": "^1.18.0",
		"tslint-eslint-rules": "^5.4.0",
		"tslint-microsoft-contrib": "^6.2.0",
		"tslint-plugin-prettier": "^2.3.0",
		"typescript": "^4.1.2",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"vsce": "^1.81.1",
		"vscode-test": "^1.4.0",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"@azure/msal-node": "^1.1.0",
		"@jupyterlab/services": "^5.2.5",
		"fs-extra": "^9.0.1",
		"get-port": "^5.1.1",
		"inversify": "^5.0.1",
		"inversify-inject-decorators": "^3.1.0",
		"lodash": "^4.17.20",
		"moment": "^2.29.1",
		"node-fetch": "^2.6.1",
		"path": "^0.12.7",
		"prettier": "2.2.1",
		"reflect-metadata": "^0.1.13",
		"request": "^2.88.2",
		"uuid": "^8.3.2",
		"vscode-extension-telemetry": "^0.1.6",
		"winston": "^3.3.3",
		"winston-transport": "^4.4.0",
		"ws": "^7.4.1"
	},
	"__metadata": {
		"id": "127cf10a-2f9b-4685-ae97-09b1d3200338",
		"publisherId": "ac8eb7c9-3e59-4b39-8040-f0484d8170ce",
		"publisherDisplayName": "Microsoft",
		"installedTimestamp": 1637003873574
	}
}