{
    "type": "object",
    "description": "Azure Kubernetes Service",
    "properties": {
        "properties": {
            "type": "object",
            "description": "Compute properties",
            "properties": {
                "computeType": {
                    "type": "string",
                    "description": "The type of the compute",
                    "const": "AKS"
                },
                "resourceId": {
                    "type": "string",
                    "description": "The Azure resource ID for the compute resource being attached"
                },
                "properties": {
                    "type": "object",
                    "description": "AKS Compute properties",
                    "properties": {
                        "clusterFqdn": {
                            "type": "string",
                            "description": "[Optional] Cluster full qualified domain name"
                        },
                        "clusterPurpose": {
                            "type": "string",
                            "description": "[Optional] Targeted usage of the cluster. This is used to provision AzureML components to\nensure the desired level of fault-tolerance and QoS. AksCompute.ClusterPurpose class is provided for\nconvenience of specifying available values. More detailed information of these values and their use cases\ncan be found here: https://aka.ms/azureml-create-attach-aks"
                        },
                        "sslConfiguration": {
                            "type": "object",
                            "description": "[Optional] SSL configuration",
                            "properties": {
                                "cert": {
                                    "type": "string",
                                    "description": "Cert data"
                                },
                                "cname": {
                                    "type": "string",
                                    "description": "CNAME of the cert"
                                },
                                "key": {
                                    "type": "string",
                                    "description": "Key data"
                                },
                                "status": {
                                    "type": "string",
                                    "description": "Enable or disable SSL for scoring",
                                    "default": "Disabled",
                                    "enum": [
                                        "Disabled",
                                        "Enabled",
                                        "Auto"
                                    ]
                                },
                                "leafDomainLabel": {
                                    "type": "string"
                                },
                                "overwriteExistingDomain": {
                                    "type": "boolean",
                                    "default": "false"
                                }
                            }
                        }
                    }
                }
            },
            "required": [
                "computeType",
                "resourceId"
            ]
        }
    },
    "required": [
        "properties"
    ]
}
